{
  "expo": {
    "name": "KAS Jewellery Employee",
    "slug": "KasHrms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/KASEmpLogo.png",
    "scheme": "kashrms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kasemployee"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/KASEmpLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kasemployee"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/KASEmpLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/KASEmpLogo.png",
          "imageWidth": 210,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98da5af2-4a84-4eaa-9528-46861b536c05"
      }
    }
  }
}