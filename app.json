{
  "expo": {
    "name": "KAS Employee",
    "slug": "KasHrms",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/images/KASEmpLogo.png",
    "scheme": "kashrms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "signingConfig": {
      "keystorePath": "./kasemployee.jks",
      "keystorePassword": "kasemployee",
      "keyAlias": "kasemployee",
      "keyPassword": "kasemployee"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kasemployee",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to take photos",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access to upload images",
        "NSMicrophoneUsageDescription": "This app needs microphone access to record audio"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/KASEmpLogo.png",
        "backgroundImage": "./assets/images/KASEmpLogo.png",
        "monochromeImage": "./assets/images/KASEmpLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "kas.employee",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/KASEmpLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/KASEmpLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98da5af2-4a84-4eaa-9528-46861b536c05"
      }
    }
  }
}